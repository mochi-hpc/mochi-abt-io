@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

find_dependency(PkgConfig)
pkg_check_modules(ARGOBOTS REQUIRED IMPORTED_TARGET argobots)
pkg_check_modules(JSONC REQUIRED IMPORTED_TARGET json-c)

set(_abt_io_enable_bedrock @ENABLE_BEDROCK@)
set(_abt_io_enable_liburing @ENABLE_LIBURING@)

if(_abt_io_enable_liburing)
    pkg_check_modules(LIBURING REQUIRED IMPORTED_TARGET liburing)
endif()

if(_abt_io_enable_bedrock)
    pkg_check_modules(BEDROCK REQUIRED IMPORTED_TARGET bedrock-module-api)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/abt-io-targets.cmake")

check_required_components(abt-io)
